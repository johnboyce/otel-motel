meta {
  name: Get Bookings by Customer
  type: http
  seq: 3
}

post {
  url: http://localhost:8080/graphql
  body: graphql
  auth: none
}

body:graphql {
  query {
    bookingsByCustomer(customerId: 1) {
      id
      checkInDate
      checkOutDate
      numberOfGuests
      totalPrice
      status
      room {
        roomNumber
        hotel {
          name
          city
        }
      }
    }
  }
}
