meta {
  name: Get Upcoming Bookings
  type: graphql
  seq: 2
}

post {
  url: http://localhost:8080/graphql
  body: graphql
  auth: none
}

body:graphql {
  query {
    upcomingBookings {
      id
      roomId
      customerId
      checkInDate
      checkOutDate
      numberOfGuests
      totalPrice
      status
    }
  }
}
