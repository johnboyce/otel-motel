meta {
  name: Get All Hotels
  type: graphql
  seq: 1
}

post {
  url: {{graphql_url}}
  body: graphql
  auth: inherit
}

body:graphql {
  query {
    hotels {
      id
      name
      address
      city
      country
      description
      starRating
    }
  }
}

docs {
  # Get All Hotels
  
  This query is public and does not require authentication.
  However, the token will be sent if available in the environment.
}
